page{
	padding-bottom: 116rpx;
}
.pageBg{
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  background: url(https://636c-cloud1-4gojwhjp38f8a41c-1315829236.tcb.qcloud.la/icon/pageBg.png) no-repeat, linear-gradient(180deg, #F5EFFF 0%, #FFFFFF 100%);
  background-size: 100%;
  z-index: -1;
}
.searchBar{
  width: 100%;
  height: 120rpx;
  padding: 20rpx 32rpx;
  position: fixed;
  z-index: 9;
  background: url(https://636c-cloud1-4gojwhjp38f8a41c-1315829236.tcb.qcloud.la/icon/pageBg.png) no-repeat, #F5EFFF;
  background-size: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.searchBar>view:first-child{
  position: relative;
}
.searchBar>view:last-child{
  font-size: 30rpx;
  color: #370092;
}
.searchBar>view:first-child input{
  width: 596rpx;
  height: 80rpx;
  background: #FFFFFF;
  box-shadow: 0px 3px 6px 1px rgba(0,47,118,0.08);
  border-radius: 48rpx;
  padding: 0 84rpx;
}
.searchBar>view:first-child van-icon{
  position: absolute;
  top: 50%;
  left: 34rpx;
  transform: translateY(-50%);
}
.orderList{
  padding-top: 20rpx;
  width: 686rpx;
  margin: 0 auto;
}
.orderItem{
  display: flex;
  align-items: center;
  border-bottom: 2rpx solid #EFF4FF;
  padding: 30rpx 0;
}
.userAvatar,.orderCover{
  display: flex;
  align-items: center;
  font-size: 22rpx;
  line-height: 22rpx;
  color: #999999;
}
.userAvatar text:nth-child(2){
  display: inline-block;
  margin: 0 6rpx;
}
.orderCover image{
  width: 156rpx;
  height: 160rpx;
  background: #FFFFFF;
  box-shadow: 0px 3px 6px 1px rgba(0,47,118,0.08);
  border-radius: 30rpx;
  margin-right: 20rpx;
}
.userAvatar image{
  width: 44rpx;
  height: 44rpx;
  background: #FFFFFF;
  border-radius: 50%;
}
.orderTime{
  font-size: 22rpx;
  line-height: 22rpx;
  color: #999999;
  margin: 16rpx 0;
  max-width: 300rpx;
}
.orderName{
  font-size: 30rpx;
  line-height: 30rpx;
  color: #3A454B;
  font-weight: bold;
}
.orderStatus{
  font-size: 28rpx;
  line-height: 28rpx;
}
.orderPrice{
  margin-right: 0;
  margin-left: auto;
  margin-top: 20rpx;
  text-align: right;
}
.orderPrice>view:last-child{
  font-size: 28rpx;
  margin-top: 56rpx;
}
.orderPrice>view:last-child text{
  font-size: 36rpx;
  font-weight: bold;
}
.weui-loadmore {
  width: 100%;
  height: 50rpx;
  font-size: 30rpx;
  text-align: center;
}

.weui-loading {
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.weui-loadmore-tips {
  display: inline-block;
  vertical-align: middle;
}