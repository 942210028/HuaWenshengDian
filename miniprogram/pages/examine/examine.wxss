page{
  background: url(https://636c-cloud1-4gojwhjp38f8a41c-1315829236.tcb.qcloud.la/icon/pageBg.png) no-repeat, linear-gradient(180deg, #F5EFFF 0%, #FFFFFF 100%) no-repeat;
  background-size: 100%;
	padding-bottom: 116rpx;
}
.rankingBox{
	width: 686rpx;
	height: 460rpx;
	background: url(https://636c-cloud1-4gojwhjp38f8a41c-1315829236.tcb.qcloud.la/icon/rankingBoxBg.png) no-repeat, #b81d1d;
	background-size: 270rpx 132rpx;
	background-position: 288rpx 50rpx;
	border-radius: 30rpx;
	color: white;
	margin: 0 auto;
	padding: 20rpx;
}
.rankingBox>.title{
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: space-between;
	padding: 10rpx 34rpx 30rpx;
	font-size: 26rpx;
}
.rankingBox>.title>view:first-child{
	font-size: 34rpx;
	font-weight: bold;
}
.rankingList{
	width: 646rpx;
	height: 326rpx;
	background-color: white;
	background: #FFFFFF;
	box-shadow: 0px 3px 6px 1px rgba(0,47,118,0.08);
	border-radius: 18rpx;
	padding: 38rpx 30rpx;
}
.rankingItem{
	width: 100%;
	height: 82rpx;
	display: flex;
	align-items: center;
	color: #728A97;
	font-size: 22rpx;
}
.rankingItem image{
	width: 44rpx;
	height: 40rpx;
	margin-right: 14rpx;
}
.rankingItem>view:nth-child(2){
	margin-right: 14rpx;
}
.rankingItem>view:nth-child(2)>view:first-child{
	font-size: 28rpx;
	color: #3A454B;
}
.match{
	width: 686rpx;
	height: 334rpx;
	background: #FFFFFF;
	box-shadow: 0px 3px 6px 1px rgba(0,47,118,0.08);
	border-radius: 30rpx;
	margin: 26rpx auto;
	padding: 30rpx;
}
.matchTitle{
	display: flex;
	align-items: center;
	justify-content: space-between;
	color: #b81d1d;
	font-weight: bold;
	font-size: 30rpx;
	line-height: 30rpx;
	margin-top: 12rpx;
}
.joinUser{
	display: flex;
	align-items: center;
	justify-content: space-between;
	font-size: 20rpx;
	color: #999999;
}
.joinUser>view{
	display: flex;
	align-items: center;
}
.joinUser>view>image{
	width: 66rpx;
	height: 66rpx;
	border-radius: 50%;
	margin-right: 10rpx;
}
.joinUser>view:nth-child(1)>view{
	display: inline-block;
	width: 66rpx;
	height: 66rpx;
	line-height: 60rpx;
	text-align: center;
	background: #F0F1F1;
	border-radius: 32rpx;
	box-shadow: 0px 3px 6px 1px rgba(0,47,118,0.08);
	font-weight: bolder;
	font-size: 48rpx;
	color: #505050;
}
.matchSubTitle{
	font-size: 40rpx;
	line-height: 40rpx;
	font-weight: bold;
	color: #3A454B;
	margin-top: 40rpx;
}
.matchDetail{
	display: flex;
	align-items: center;
	justify-content: space-between;
	font-size: 24rpx;
	margin-top: 12rpx;
}
.matchDetail>view:nth-child(2){
	width: 172rpx;
	height: 66rpx;
	color: white;
	background: #b81d1d;
	box-shadow: 0px 3px 6px 1px rgba(0,47,118,0.08);
	border-radius: 54rpx;
	line-height: 66rpx;
	text-align: center;
	font-size: 26rpx;
}