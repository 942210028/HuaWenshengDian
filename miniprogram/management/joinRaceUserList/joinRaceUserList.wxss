page{
  background: url(https://636c-cloud1-4gojwhjp38f8a41c-1315829236.tcb.qcloud.la/icon/pageBg.png) no-repeat, linear-gradient(180deg, #F5EFFF 0%, #FFFFFF 100%) no-repeat;
  background-size: 100%;
	padding-bottom: 116rpx;
}
.seachbox{
	width: 100%;
	position: fixed;
	display: flex;
	align-items: center;
	justify-content: space-between;
	padding: 18rpx 32rpx;
  background: url(https://636c-cloud1-4gojwhjp38f8a41c-1315829236.tcb.qcloud.la/icon/pageBg.png) no-repeat, #F5EFFF no-repeat;
  background-size: 100%;
	z-index: 2;
}
.weui-loadmore {
  width: 100%;
  height: 50rpx;
  font-size: 30rpx;
  text-align: center;
}

.weui-loading {
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.weui-loadmore-tips {
  display: inline-block;
  vertical-align: middle;
}
.input{
	width: 596rpx;
	height: 82rpx;
	background: #FFFFFF;
	box-shadow: 0px 3px 6px 1px rgba(0,47,118,0.08);
	border-radius: 48rpx;
	padding: 24rpx 24rpx 24rpx 84rpx;
	position: relative;
}
.input>input{
	width: 470rpx;
	height: 34rpx;
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
}
.input>van-icon{
	position: absolute;
	top: 50%;
	left: 34rpx;
	transform: translateY(-50%);
	color: #999999;
	font-size: 34rpx;
}
.seachConfirm{
	color: #370092;
	font-size: 30rpx;
}
.userList_box{
	width: 686rpx;
	margin: 0 auto;
}
.userAvatar{
	display: flex;
	margin-right: 26rpx;
}
.userAvatar>image{
	width: 72rpx;
	height: 72rpx;
	border-radius: 50%;
}
.userList>view{
	display: flex;
	align-items: flex-end;
	justify-content: space-between;
	padding: 30rpx 0;
	border-bottom: 1px solid #FFFFFF;
	position: relative;
}
.userList>view>view:last-child{
	color: #370092;
	font-size: 26rpx;
}
.userListItem{
	display: flex;
	align-items: center;
}
.userInfo>view:first-child{
	font-size: 30rpx;
}
.userInfo>view:last-child{
	font-size: 24rpx;
}