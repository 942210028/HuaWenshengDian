.page{
  display: block;
  min-height: 100%;
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
	background: linear-gradient(180deg, #F2F6F8, #E8F2FB) no-repeat;
	padding-bottom: 116rpx;
}
.cover{
	width: 100%;
  height: 642rpx;
  filter: blur(30rpx);
}
.cover>image{
	width: 100%;
	height: 100%;
}
.magazineCover{
  display: flex;
}
.magazineCover image{
  width: 258rpx;
  height: 346rpx;
}
.magazine{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.magazineDetail{
  width: 686rpx;
  margin: -400rpx auto 0;
  padding: 32rpx;
  background: #FFFFFF;
  position: relative;
  z-index: 2;
  border-radius: 24rpx;
}
.magazineInfo{
  width: 322rpx;
}
.magazineInfo>view:nth-child(1){
  font-size: 28rpx;
  line-height: 50rpx;
  color: #000000;
  font-weight: 600;
}
.magazineInfo>view:nth-child(2){
  font-size: 28rpx;
  line-height: 50rpx;
  color: #000000;
  font-weight: 400;
}
.magazineInfo>view:nth-child(3){
  font-size: 22rpx;
  line-height: 36rpx;
  color: #999999;
}
.magazineInfo>view:nth-child(4){
  font-size: 28rpx;
  line-height: 80rpx;
  color: #F7540F;
}
.magazineDetail .title{
	margin: 60rpx auto 30rpx 0;
	font-size: 36rpx;
}
.title{
	margin: 60rpx auto 30rpx 64rpx;
	font-size: 36rpx;
}
.content{
	font-size: 28rpx;
	line-height: 40rpx;
	color: #666666;
	margin: 0 auto;
}
.chooseGoods{
	width: 686rpx;
	height: 270rpx;
	margin: 0 auto;
	background: #FFFFFF;
	border-radius: 18px;
	padding: 32rpx;
	display: flex;
	align-items: center;
}
.goodsInfo{
	margin-left: 50rpx;
	font-size: 28rpx;
	line-height: 50rpx;
}
.goodsInfo>view:nth-child(2){
	font-size: 22rpx;
	color: #666666;
}
.goodsInfo>view:nth-child(3){
	color: #ED5708;
}
.goodsInfo>view:nth-child(4){
  width: 322rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 16rpx;
}
.goodsInfo>view:nth-child(4) view{
  width: 94rpx;
  height: 38rpx;
  border-radius: 20rpx;
  background: #BBE2FB;
  font-size: 24rpx;
  text-align: center;
  line-height: 38rpx;
  color: white;
}
.selectNumber{
  border: 2rpx solid #369AFF;
  background: white !important;
  color: #666666 !important;
}
.chooseGoods_cover{
	width: 208rpx;
	height: 206rpx;
	background: #FFFFFF;
	box-shadow: 0px 2px 10px 2px rgba(0,0,0,0.05);
	border-radius: 10rpx;
	overflow: hidden;
	position: relative;
}
.chooseGoods_cover>view:first-child {
	text-align: center;
	margin-top: 16rpx;
}
.chooseGoods_cover>view:first-child image{
	width: 96rpx;
	height: 114rpx;
}
.chooseGoods_cover>view:last-child{
	position: absolute;
	overflow: hidden;
	bottom: 0;
	width: 100%;
	height: 64rpx;
	line-height: 64rpx;
	text-align: center;
	font-size: 24rpx;
	color: #333333;
	background: linear-gradient(241deg, #C1E7FB 0%, #A4CFFB 100%);
}
.chooseGoods_cover>view:last-child>view{
	position: absolute;
	right: -80%;
	bottom: -80%;
	width: 100%;
	height: 128rpx;
	font-size: 20rpx;
	line-height: 40rpx;
	color: white;
	background: #369AFF;
	transform: rotate(135deg);
}
.chooseGoods_cover>view:last-child>view>view{
	transform: rotate(-135deg) translate(-50%,-90%);
}
.buttomTitle{
	margin: 60rpx auto 30rpx 64rpx;
	font-size: 28rpx;
}
.buttomContent{
	font-size: 24rpx;
	line-height: 40rpx;
	color: #666666;
	width: 622rpx;
	margin: 0 auto;
}
.owned{
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
	margin-top: 87rpx;
	font-size: 30rpx;
}
.commit{
	width: 476rpx;
	height: 112rpx;
	background: #369AFF;
	border-radius: 58rpx;
	font-size: 36rpx;
	line-height: 112rpx;
	color: white;
	text-align: center;
	margin: 82rpx auto 0;
}