.page{
	width: 100%;
	height: 100vh;
  position: fixed;
  left: 0;
	top: 0;
	z-index: -1;
  background: url(https://636c-cloud1-4gojwhjp38f8a41c-1315829236.tcb.qcloud.la/icon/entrancePageBg.png) no-repeat;
  background-size: 100% 100%;
}
.logo{
  width: 100%;
  text-align: center;
}
.logo image{
  width: 686rpx;
  height: 310rpx;
  margin-top: 52rpx;
}
.userList{
  margin: 56rpx auto;
  width: 580rpx;
  height: 244rpx;
  background: #FFFFFF;
  box-shadow: 0px 3px 6px 1px rgba(0,47,118,0.08);
  border-radius: 36rpx;
  display: flex;
  align-items: center;
  justify-content: space-evenly;
}
.userList>view:nth-child(2){
  text-align: center;
}
.userList>view:nth-child(1){
  display: flex;
}
.userList image{
  width: 0;
  height: 128rpx;
}
.user{
  font-size: 42rpx;
  font-weight: bold;
}
.tapEntrance{
  color: #b81d1d;
  font-size: 24rpx;
}